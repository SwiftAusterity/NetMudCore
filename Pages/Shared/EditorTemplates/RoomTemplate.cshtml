@using NetMudCore.Data.Room

@model RoomTemplate

<h3 title="Parent Locale">Locale - @Model.ParentLocation.Name</h3>
<hr />
@Html.HiddenFor(m => m.ParentLocation)

<h4>Room Details</h4>
<div class="form-group row">
    @Html.EditorFor(m => m.Name)
    @Html.EditorFor(m => m.Medium, new { validItems = ViewData["ValidMaterials"] })
</div>

@Html.EditorFor(m => m.Model)
