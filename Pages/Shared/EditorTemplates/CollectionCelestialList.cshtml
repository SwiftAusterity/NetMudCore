@using NetMudCore.Models
@using NetMudCore.DataStructure.Gaia
@model HashSet<ICelestial>
@{
    var currentCount = Model.Count();
    var rowsName = ViewData.ModelMetadata.PropertyName + "Rows";
}
<div id="@rowsName" class="iterativeContainer">
    <div class="addRowContainer">
        <div>
            <a href="#" class="addRow btn" data-current-row="@currentCount" onclick="javascript: replacePart(this, newCelestialDropdown, @currentCount, '@rowsName'); return false;"><span class="addWord">Add Row</span></a>
        </div>
    </div>
    @if (Model.Count() > 0)
    {
        @Html.EditorForMany(m => m, null, 0, "CollectionCelestialListItem")
    }
</div>
<script type="text/javascript">
    var newCelestialDropdown = decodeHtml('@Html.EmptyEditorForMany(m => m, null, currentCount, "CollectionCelestialListItem")');
</script>
