@model NetMudCore.Models.Admin.ManageCombosViewModel
@{
    ViewBag.Breadcrumbs = new Tuple<string, string[], string[]>("Combos", new string[] { "Dashboard" }, new string[] { "/Manage" });

    var linkText = Html.ActionLink("99999999", "EditCombos", new { id = 99999999 }).ToString();
    var tableColumns = new Dictionary<string, Func<dynamic, string>>();
    tableColumns.Add("Name", obj => linkText.Replace("99999999", obj.Name.ToString()));
}
    
@Html.Partial("ListTable", new NetMudCore.Models.ListTableViewModel() { AddMethodName = "AddCombos", EntityTypeName = "FightingArtCombination", Name = "Combos", Columns = tableColumns, Values = Model.CurrentPageOfItems, IncludeDelete = false, IncludeUnapprove = false })

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}